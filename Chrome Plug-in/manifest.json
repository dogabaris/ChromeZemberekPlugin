{
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "anasayfa.html",
      "default_title": "Kelime Kök Sayıcı"
   },
   "content_scripts": [ {
      "all_frames": false,
     
      "js": [  "textislemleri.js","angular.min.js","jquery.min.js" ,"hilitor-utf8.js"],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],

   "description": "Kök Sayıcı",
   "icons": {
      "48": "icon.png"
   },
   "manifest_version": 2,
   "name": "KelimeSayıcı",
   "permissions": ["activeTab", "contextMenus", "notifications", "tabs","background", "http://*/*", "https://*/*" ],
   "version": "1.0.0"
}